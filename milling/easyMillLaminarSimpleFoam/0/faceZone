FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       surfaceScalarField;
    location    "0";
    object      faceZone;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform -1;

boundaryField
{
    rotor
    {
        type            calculated;
        value           uniform -1;
    }
    box1_wall
    {
        type            calculated;
        value           uniform -1;
    }
    outlet
    {
        type            calculated;
        value           uniform -1;
    }
    box2_wall
    {
        type            calculated;
        value           uniform -1;
    }
    box3_wall
    {
        type            calculated;
        value           uniform -1;
    }
    stator1_wall
    {
        type            calculated;
        value           uniform -1;
    }
    stator2_wall
    {
        type            calculated;
        value           uniform -1;
    }
    stator3_wall
    {
        type            calculated;
        value           uniform -1;
    }
    nozzle_wall
    {
        type            calculated;
        value           uniform -1;
    }
    inlet
    {
        type            calculated;
        value           uniform -1;
    }
    axis
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    bottomEmptyFaces
    {
        type            wedge;
        value           uniform -1;
    }
    topEmptyFaces
    {
        type            wedge;
        value           uniform -1;
    }
}


